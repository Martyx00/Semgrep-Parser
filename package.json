{
  "name": "semgrepparser",
  "displayName": "SemgrepParser",
  "description": "Extension to parse JSON output of the Semgrep scans.",
  "version": "1.0.0",
  "repository": "https://github.com/Martyx00/Semgrep-Parser",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "semgrep-triage.openResults",
        "title": "Semgrep Triage: Open Results File"
      },
      {
        "command": "semgrep-triage.openEmpty",
        "title": "Semgrep Triage: Open Empty View"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
